# AtividadeComC7

#include <stdio.h>
#include <stdlib.h>

int main () {
	
	float pesoEmKilo;
	
		
	printf("digite o peso do seu prato em kilos\n");
	scanf("%f", &pesoEmKilo);
	
	float valorPorKilo = 45;
	
	
// Calculo para saber o pre√ßo do prato	
	float valorTotal = pesoEmKilo * valorPorKilo; 
	
	printf("\no seu peso em gramas e %f", valorTotal);

	return 0;
	
}
